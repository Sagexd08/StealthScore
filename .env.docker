# Docker Environment Configuration for Stealth Score
# Copy this file to .env and fill in your actual values

# OpenRouter API Key (REQUIRED)
OPENROUTER_API_KEY=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

# Application Configuration
APP_NAME=Stealth Score
APP_VERSION=2.0.0
ENVIRONMENT=production

# Network Configuration
FRONTEND_PORT=3000
BACKEND_PORT=8000
REDIS_PORT=6379

# CORS Configuration
CORS_ORIGINS=http://localhost:3000

# Feature Flags
ENABLE_TEE=true
ENABLE_ZK_PROOFS=true
ENABLE_FEDERATED_LEARNING=true
ENABLE_WEB3=true

# Database Configuration (if needed)
DATABASE_URL=sqlite:///./stealthscore.db

# Redis Configuration
REDIS_URL=redis://redis:6379

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
